<% include partials/header %>
<div class="container my-3">
  <a href="/pagrindinis"><button type="button" class="btn btn-light">Grįžti</button></a>
  <h1 class="text-center"><%= result[0].pavadinimas %></h1>
  <h3>Papildymas</h3>
  <div>
    <form action="/apskaita/<%= result[0].tasko_ID %>" method="post">
        <div class="form-inline">
          <label for="minkstiledai" class="mr-3">Ledų masė, kg</label>
          <input type="number" class="form-control" name="minkstiledai" id="minkstiledai" step="0.01">
        </div>
        <div class="form-inline my-3">
          <label for="kabinamiledaivnt" class="mr-3">Kabinamų dėžutės, vnt</label>
          <input type="number" class="form-control" name="kabinamiledaivnt" id="kabinamiledaivnt" step="1">
        </div>
        <div class="form-inline my-3">
          <label for="kabinamiledai" class="mr-3">Kabinami ledai, kg</label>
          <input type="number" class="form-control" name="kabinamiledai" id="kabinamiledai" step="0.01">
        </div>
        <div class="form-inline my-3">
          <label for="serbetas" class="mr-3">Šerbetas, l</label>
          <input type="number" class="form-control" name="serbetas" id="serbetas" step="0.01">
        </div>
        <div class="form-inline my-3">
          <label for="stiklines02" class="mr-3">Stiklinės 0.2, vnt</label>
          <input type="number" class="form-control" name="stiklines02" id="stiklines02" step="1">
        </div>
        <div class="form-inline my-3">
          <label for="stiklines03" class="mr-3">Stiklinės 0.3, vnt</label>
          <input type="number" class="form-control" name="stiklines03" id="stiklines03" step="1">
        </div>
        <div class="form-inline my-3">
          <label for="stiklines05" class="mr-3">Stiklinės 0.5, vnt</label>
          <input type="number" class="form-control" name="stiklines05" id="stiklines05" step="1">
        </div>
        <% if (result[0].pavadinimas == 'Palanga' || result[0].pavadinimas == 'Zydroji' || result[0].pavadinimas == 'Sventes'){%>
          <div class="form-inline my-3">
              <label for="kava" class="mr-3">Kava, kg</label>
              <input type="number" class="form-control" name="kava" id="kava" step="1">
            </div>
        <% }%>
        <% if (result[0].pavadinimas == 'Centras' || result[0].pavadinimas == 'Zydroji'){%>
          <div class="form-inline my-3">
              <label for="vafliutesla" class="mr-3">Vaflių tešla, kg</label>
              <input type="number" class="form-control" name="vafliutesla" id="vafliutesla" step="1">
            </div>
        <% }%>
        <% if (result[0].pavadinimas == 'Banga' || result[0].pavadinimas == 'Zydroji' || result[0].pavadinimas == 'Palanga' || result[0].pavadinimas == 'Sventes'){%>
        <div class="form-inline my-3">
          <label for="gerimai1" class="mr-3">Tiche 0.5l, Neptūnas 0.5l gazuotas/negazuotas, vnt</label>
          <input type="number" class="form-control" name="gerimai1" id="gerimai1" step="1">
        </div>
        <div class="form-inline my-3">
          <label for="gerimai5" class="mr-3">Coca cola, Sprite, Fanta 0.5l, Tiche 1l, vnt</label>
          <input type="number" class="form-control" name="gerimai5" id="gerimai5" step="1">
        </div>
        <div class="form-inline my-3">
          <label for="gerimai2" class="mr-3">Tiche 1.5l, Neptūnas 1.5l, vnt</label>
          <input type="number" class="form-control" name="gerimai2" id="gerimai2" step="1">
        </div>
        <% }%>
        <input type="submit" value="Pateikti">
    </form>
    </div>
    <div class="mt-5">
      <h3>Visa Apskaita</h3>
      <table class="table table-striped" id="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Gauta pradžioje</th>
            <th scope="col">Atvežta</th>
            <th scope="col">Suprekiauta</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <th scope="row">Minkšti ledai, kg</th>
            <td><%= result[0].gauta_minkstiledai %></td>
            <td><%= result[0].minkstiledai %></td>
            <td><%= (Number(result[0].gauta_minkstiledai) + Number(result[0].minkstiledai))*10 %> &euro;</td>
          </tr>
          <tr>
            <th scope="row">Kabinamų dėžutės, vnt</th>
            <td><%= result[0].gauta_kabinamiledaivnt %></td>
            <td><%= result[0].kabinamiledaivnt %></td>
            <td><%= (Number(result[0].gauta_kabinamiledaivnt) + Number(result[0].kabinamiledaivnt))*55 %> &euro;</td>
          </tr>
          <tr>
            <th scope="row">Kabinami ledai, kg</th>
            <td><%= result[0].gauta_kabinamiledai %></td>
            <td><%= result[0].kabinamiledai %></td>
            <td><%= (Number(result[0].gauta_kabinamiledai) + Number(result[0].kabinamiledai))* 19.64 %> &euro;</td>
          </tr>
          <tr>
            <th scope="row">Šerbetas, l</th>
            <td><%= result[0].gauta_serbetas %></td>
            <td><%= result[0].serbetas %></td>
            <td><%= (Number(result[0].gauta_serbetas) + Number(result[0].serbetas))*30 %> &euro;</td>
          </tr>
          <tr>
            <th scope="row">Stiklinės 0.2, vnt</th>
            <td><%= result[0].gauta_stiklines02 %></td>
            <td><%= result[0].stiklines02 %></td>
            <td><%= (Number(result[0].gauta_stiklines02) + Number(result[0].stiklines02))*1.2 %> &euro;</td>
          </tr>
          <tr>
            <th scope="row">Stiklinės 0.3, vnt</th>
            <td><%= result[0].gauta_stiklines03 %></td>
            <td><%= result[0].stiklines03 %></td>
            <td><%= (Number(result[0].gauta_stiklines03) + Number(result[0].stiklines03))*1.5 %> &euro;</td>
          </tr>
          <tr>
            <th scope="row">Stiklinės 0.5, vnt</th>
            <td><%= result[0].gauta_stiklines05 %></td>
            <td><%= result[0].stiklines05 %></td>
            <td><%= (Number(result[0].gauta_stiklines05) + Number(result[0].stiklines05))*2 %> &euro;</td>
          </tr>
          <% if (result[0].pavadinimas == 'Palanga' || result[0].pavadinimas == 'Zydroji' || result[0].pavadinimas == 'Sventes'){%>
            <tr>
              <th scope="row">Kava, kg</th>
              <td><%= result[0].gautaKava %></td>
              <td><%= result[0].kava %></td>
              <td><%= (Number(result[0].gautaKava) + Number(result[0].kava))*160 %> &euro;</td>
            </tr>
          <% }%>
          <% if (result[0].pavadinimas == 'Centras' || result[0].pavadinimas == 'Zydroji'){%>
            <tr>
              <th scope="row">Vaflių tešla, kg</th>
              <td><%= result[0].gautaVafliutesla %></td>
              <td><%= result[0].vafliutesla %></td>
              <td><%= (Number(result[0].gautaVafliutesla) + Number(result[0].vafliutesla))*30 %> &euro;</td>
            </tr>
          <% }%>
          <% if (result[0].pavadinimas == 'Banga' || result[0].pavadinimas == 'Zydroji' || result[0].pavadinimas == 'Palanga' || result[0].pavadinimas == 'Sventes'){%>
          <tr>
            <th scope="row">Tiche 0.5l, Neptunas 0.5l gazuotas/negazuotas, vnt</th>
            <td><%= result[0].gautaGerimai1 %></td>
            <td><%= result[0].gerimai1 %></td>
            <td><%= (Number(result[0].gautaGerimai1) + Number(result[0].gerimai1))*1 %> &euro;</td>
          </tr>
          <tr>
            <th scope="row">Coca cola, Sprite, Fanta, vnt</th>
            <td><%= result[0].gautaGerimai15 %></td>
            <td><%= result[0].gerimai15 %></td>
            <td><%= (Number(result[0].gerimai15) + Number(result[0].gautaGerimai15))*1.5 %> &euro;</td>
          </tr>
          <tr>
            <th scope="row">Tiche 1.5l, Neptūnas 1.5l, vnt</th>
            <td><%= result[0].gautaGerimai2 %></td>
            <td><%= result[0].gerimai2 %></td>
            <td><%= (Number(result[0].gerimai2) + Number(result[0].gautaGerimai2))*2 %> &euro;</td>
          </tr>
          <% }%>
        </tbody>
      </table>
      <span class="font-weight-bold" id="val"></span>
      <span class="font-weight-bold" id="val1"></span>
  </div>
  <div>
    <h1>Tarpinės ataskaitos</h1>
    <form action="/nuemimas/<%= result[0].tasko_ID %>" method="post">
      <div class="form-inline">
        <label for="nuimtaMinksti" class="mr-3">Ledų masė, kg</label>
        <input type="number" class="form-control" name="nuimtaMinksti" id="nuimtaMinksti" min="0.01" step="0.01">
      </div>
      <div class="form-inline my-3">
        <label for="nuimtaKabinami" class="mr-3">Kabinami ledai, kg</label>
        <input type="number" class="form-control" name="nuimtaKabinami" id="nuimtaKabinami" min="0.01" step="0.01">
      </div>
      <div class="form-inline my-3">
        <label for="nuimtaKabinamiledaivnt" class="mr-3">Kabinamų dėžutės, vnt</label>
        <input type="number" class="form-control" name="nuimtaKabinamiledaivnt" id="nuimtaKabinamiledaivnt" min="1" step="1">
      </div>
      <div class="form-inline my-3">
        <label for="nuimtaSerbetas" class="mr-3">Šerbetas, l</label>
        <input type="number" class="form-control" name="nuimtaSerbetas" id="nuimtaSerbetas" min="0.01" step="0.01">
      </div>
      <div class="form-inline my-3">
        <label for="nuimtaStiklines02" class="mr-3">Stiklinės 0.2, vnt</label>
        <input type="number" class="form-control" name="nuimtaStiklines02" id="nuimtaStiklines02" min="1" step="1">
      </div>
      <div class="form-inline my-3">
        <label for="nuimtaStiklines03" class="mr-3">Stiklinės 0.3, vnt</label>
        <input type="number" class="form-control" name="nuimtaStiklines03" id="nuimtaStiklines03" min="1" step="1">
      </div>
      <div class="form-inline my-3">
        <label for="nuimtaStiklines05" class="mr-3">Stiklinės 0.5, vnt</label>
        <input type="number" class="form-control" name="nuimtaStiklines05" id="nuimtaStiklines05" min="1" step="1">
      </div>
      <% if (result[0].pavadinimas == 'Palanga' || result[0].pavadinimas == 'Zydroji' || result[0].pavadinimas == 'Sventes'){%>
        <div class="form-inline my-3">
            <label for="nuimtaKava" class="mr-3">Kava, kg</label>
            <input type="number" class="form-control" name="nuimtaKava" id="nuimtaKava" step="1">
          </div>
      <% }%>
      <% if (result[0].pavadinimas == 'Centras' || result[0].pavadinimas == 'Zydroji'){%>
        <div class="form-inline my-3">
            <label for="nuimtaVafliutesla" class="mr-3">Vaflių tešla, kg</label>
            <input type="number" class="form-control" name="nuimtaVafliutesla" id="nuimtaVafliutesla" step="1">
          </div>
      <% }%>
      <% if (result[0].pavadinimas == 'Banga' || result[0].pavadinimas == 'Zydroji' || result[0].pavadinimas == 'Palanga' || result[0].pavadinimas == 'Sventes'){%>
      <div class="form-inline my-3">
        <label for="nuimtaGerimai1" class="mr-3">Tiche 0.5l, Neptūnas 0.5l gazuotas/negazuotas, vnt</label>
        <input type="number" class="form-control" name="nuimtaGerimai1" id="nuimtaGerimai1" step="1">
      </div>
      <div class="form-inline my-3">
        <label for="nuimtaGerimai15" class="mr-3">Coca cola, Sprite, Fanta 0.5l, vnt</label>
        <input type="number" class="form-control" name="nuimtaGerimai15" id="nuimtaGerimai15" step="1">
      </div>
      <div class="form-inline my-3">
        <label for="nuimtaGerimai2" class="mr-3">Tiche 1.5l, Neptūnas 1.5l, vnt</label>
        <input type="number" class="form-control" name="nuimtaGerimai2" id="nuimtaGerimai2" step="1">
      </div>
      <% }%>
      <div class="form-inline my-3">
        <label for="pavadinimas" class="mr-3">Taškas</label>
        <select class="form-control" id="pavadinimas" name="pavadinimas">
          <% if (result[0].pavadinimas == 'Centras'){%>
            <option value="Centras">Centras</option>
            <% }%>
            <% if (result[0].pavadinimas == 'Banga'){%>
            <option value="Banga">Banga</option>
            <% }%>
            <% if (result[0].pavadinimas == 'Pliazas'){%>
            <option value="Pliazas">Pliažas</option>
            <% }%>
            <% if (result[0].pavadinimas == 'Zydroji'){%>
            <option value="Zydroji">Žydroji</option>
            <% }%>
            <% if (result[0].pavadinimas == 'Palanga'){%>
            <option value="Palanga">Palanga</option>
            <% }%>
            <% if (result[0].pavadinimas == 'Sventes'){%>
            <option value="Sventes">Šventės</option>
            <% }%>
        </select>
      </div>
      <div class="form-inline my-3">
        <label for="data" class="mr-3">Nuėmimo data:</label>
        <input type="date" name="data" id="data" class="form-control" required>
      </div>
      <input type="submit" value="Pateikti" id="nuimta">
    </form>
  </div>
  <div id="lenteleNuimta">
    <% if(result[0].nuimtaMinksti) {%>
