            <td><%= (Number(result.nuimta_pradziaKava) - Number(result.nuimtaKava))*160 %> &euro;</td>
          </tr>
          <% }%>
          <% if (result.pavadinimas == 'Centras' || result.pavadinimas == 'Zydroji'){%>
